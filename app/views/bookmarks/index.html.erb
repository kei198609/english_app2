<table>
  <thead>
    <tr>
      <th>投稿者名</th>
      <th>タイトル</th>
      
    </tr>
  </thead>
  <tbody>
    <% @bookmarks.each do |bookmark| %>
      <tr>
        <td><%= bookmark.post.user.name %></td>
        <td>
          <%= link_to post_path(bookmark.post) do %>
            <%= bookmark.post.scene %>
          <% end %>
        </td>

      </tr>
    <% end %>
  </tbody>
</table>